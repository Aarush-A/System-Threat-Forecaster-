    },
    "papermill": {
     "duration": 0.052738,
     "end_time": "2025-03-12T16:42:54.593677",
     "exception": false,
     "start_time": "2025-03-12T16:42:54.540939",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "        Median      Mean  IQR\n",
      "target                       \n",
      "0          4.0  3.894872  2.0\n",
      "1          4.0  4.125713  0.0\n"
     ]
    }
   ],
   "source": [
    "proc = train.groupby('target')['ProcessorCoreCount'].agg(\n",
    "    Median='median', Mean='mean',\n",
    "    Q1=lambda x: x.quantile(0.25),\n",
    "    Q3=lambda x: x.quantile(0.75)\n",
    ")\n",
    "\n",
    "proc['IQR'] = proc['Q3'] - proc['Q1']\n",
    "\n",
    "print(proc[['Median', 'Mean', 'IQR']])"
   ]
  },
